{
    "openapi": "3.0.0",
    "info": {
        "title": "File Upload API",
        "description": "API for uploading files to Google Cloud Storage",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://your-function-url.cloudfunctions.net/uploadfile"
        }
    ],
    "paths": {
        "/uploadfile": {
            "post": {
                "summary": "Upload a file to Google Cloud Storage",
                "description": "Upload a file to Google Cloud Storage",
                "consumes": [
                    "multipart/form-data"
                ],
                "parameters": [
                    {
                        "in": "formData",
                        "name": "file",
                        "description": "File to upload",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "file"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "File uploaded successfully"
                    },
                    "400": {
                        "description": "Invalid request"
                    },
                    "405": {
                        "description": "Method not allowed"
                    }
                }
            }
        }
    }
}